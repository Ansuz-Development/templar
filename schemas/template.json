{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "https://raw.githubusercontent.com/Ansuz-Development/templar/main/schemas/template.json",
  "title": "Template",
  "description": "Template schema",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "description": "Template unique identify"
    },
    "name": {
      "type": "string",
      "description": "Template name"
    },
    "items": {
      "type": "array",
      "description": "Template items",
      "items": {
        "$ref": "https://raw.githubusercontent.com/Ansuz-Development/templar/main/schemas/item.json"
      }
    }
  },
  "required": [
    "id",
    "name",
    "items"
  ]
}
